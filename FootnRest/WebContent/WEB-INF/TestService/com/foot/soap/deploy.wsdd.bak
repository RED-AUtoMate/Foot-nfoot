<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from TestService WSDL service -->

  <service name="Test" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://soap.foot.com"/>
      <parameter name="wsdlServiceElement" value="TestService"/>
      <parameter name="schemaQualified" value="http://soap.foot.com"/>
      <parameter name="wsdlServicePort" value="Test"/>
      <parameter name="className" value="com.foot.soap.TestSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Test"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="test" qname="operNS:test" xmlns:operNS="http://soap.foot.com" returnQName="retNS:testReturn" xmlns:retNS="http://soap.foot.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://soap.foot.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://soap.foot.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="test"/>

  </service>
</deployment>
